Module Module1

    Sub Main()
        Dim str1 As String

        str1 = ""

        Console.Write("Enter string: ")
        str1 = Console.ReadLine()

        If Len(str1) < 3 Then

        Else
            If Right(str1, 3) <> "ing" Then
                str1 = str1 & "ing"
            ElseIf Right(str1, 3) = "ing" Then
                str1 = str1 & "ly"
            End If
        End If
        Console.WriteLine(str1)
        Console.ReadKey()


    End Sub

End Module
