Module Module1

    Sub Main()
        Dim Str1, fleft, fright, newstr As String

        Str1 = ""
        fleft = ""
        fright = ""
        newstr = ""

        Console.Write("Enter String: ")
        Str1 = Console.ReadLine

        If Len(Str1) <= 2 Then
            Console.WriteLine(Str1)

        Else

            fleft = Left(Str1, 2)
            fright = Right(Str1, 2)
            newstr = fleft & fright

            Console.WriteLine(newstr)

        End If
        Console.ReadKey()
    End Sub

End Module
